[flake8]
exclude =
    .git,
    __pycache__,
    build,
    dist,
    versioneer.py,
    csxtools/doc/conf.py
    .ipynb_checkpoints,
    csxtools/.ipynb_checkpoints,
    csxtools/csxtools/.ipynb_checkpoints,
    csxtools/csxtools/helpers/.ipynb_checkpoints
    
max-line-length = 115
ignore = E203, W503